        "mongoose": "^5.12.2",
        "node-fetch": "^2.6.1"
    },
    "devDependencies": {
        "cross-env": "^7.0.3",
        "eslint-plugin-react-hooks": "^4.2.0",
        "jest": "^26.6.3",
        "nodemon": "^2.0.7",
        "supertest": "^6.1.3"
    }
}
